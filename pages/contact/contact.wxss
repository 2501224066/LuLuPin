.box{
  border-top: 20rpx solid #f8f8f8;
  padding-bottom: 10vh;
}

.contact {
  padding: 50rpx 30rpx 0 30rpx;
}

.contact_title {
  font-size: 40rpx;
  font-weight: 600;
  margin: 60rpx auto 0rpx auto;
  text-align: center;
}
.contact_code {
  width: 400rpx;
  height: 400rpx;
  margin: 40rpx auto;
  text-align: center;
}
.contact_number {
  font-size: 28rpx;
  font-weight: 500;
  margin: 10rpx auto;
  text-align: center;
}
.lab_copy {
  width: 70%;
  margin: 10vh 15%;
  height: 90rpx;
  line-height: 90rpx;
  color: #fff;
  text-align: center;
  background: #F74E6F;
  border-radius: 45rpx;
  font-size: 36rpx;
  display: block;
}